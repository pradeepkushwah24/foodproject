<style>
    body {
        margin: 0;
    }

    .maincontainer {
        background: #fff;
        width: 100%;
        height: 90%;
        display: flex;
        flex-direction: column;
        font-family: 'Poppins';
    }

    .box {
        width: 60%;
        height: auto;
        background: #fff;
        border-radius: 10px;
        padding: 15px;
    }

    .navbarstyle {
        width: 100%;
        height: 50px;
        background: #130f40;
    }

    .logostyle {
        width: 60px;
        height: 40px;
        margin: 5px;
        padding-left: 10px;
    }

    .leftbox {
        width: 150px;
        height: 400px;
        border-radius: 10px;
        background: #273c75;
        margin: 5px;
        padding: 5px;
    }
</style>
<html>

<head>
    <link
        href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
        rel="stylesheet">
</head>

<body>
    <div class="maincontainer">
        <div class="navbarstyle">
            <div><img src="/images/logo (2).png" class="logostyle"></div>
        </div>
        <div style="display:flex;">
            <div class="leftbox">
                <div style=" display:flex;justify-content:center ; flex-direction: column;">
                    <img src="/images/<%=data.picture %>"
                        style="  align-self:center;width:100px; height:100px; border-radius: 50px;">

                    <div style="color:#fff; font-weight: 500; letter-spacing: 1px;font-size: 12px; align-self: center;">
                        <%= data.adminname %>
                    </div>
                    <div style="color:#fff; font-weight: 500; letter-spacing: 1px;font-size: 10px; align-self: center;">
                        <%= data.eamilid %>

                    </div>
                    <div style="color:#fff; font-weight: 500; letter-spacing: 1px;font-size: 10px; align-self: center;">
                        +91 <%= data.mobileno %>

                    </div>
                    <div style="margin-top:20px; color:#fff; font-weight: 500; ;font-size: 10px; align-self: center;">
                        <a href="/food/display_all_food" target="mw" style="text-decoration: none;color:#fff ">Food
                            List</a>
                    </div>
                    <div style=" margin-top:20px;color:#fff; font-weight: 500; font-size: 10px; align-self: center;">
                        <a href="/food/food_interface" target="mw" style="text-decoration: none;color:#fff ">Add
                            Food</a>
                    </div>
                    <div style=" margin-top:20px; color:#fff; font-weight: 500; font-size: 10px; align-self: center;">
                        <a href="/food/display_all_food" target="mw" style="text-decoration: none;color:#fff "> Update
                            Food</a>
                    </div>
                    <div style=" margin-top:20px; color:#fff; font-weight: 500; font-size: 10px; align-self: center;">
                        <a href="/admin/admin_logout" style="text-decoration: none;color:#fff "> Logout</a>
                    </div>
                </div>
            </div>
            <div>
                <iframe width="1150" height="540" frameborder="0" name="mw"></iframe>
            </div>

        </div>

    </div>
</body>

</html>